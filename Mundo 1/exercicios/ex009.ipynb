   "source": [
    "Faça um programa que leia um número Inteiro qualquer e mostre na tela a sua tabuada."
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 23,
   "id": "05b8a11d",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Valor: 100\n",
      "---------------\n",
      "100 x  1 = 100\n",
      "100 x  2 = 200\n",
      "100 x  3 = 300\n",
      "100 x  4 = 400\n",
      "100 x  5 = 500\n",
      "100 x  6 = 600\n",
      "100 x  7 = 700\n",
      "100 x  8 = 800\n",
      "100 x  9 = 900\n",
      "100 x 10 = 1000\n",
      "---------------\n"
     ]
    }
   ],
   "source": [
    "n = int(input('Valor: '))\n",
    "print('-'*15)\n",
    "for i in range(10):\n",
    "    print('{} x {:2} = {}'.format(n, (i+1), (i+1)*n))\n",
    "print('-'*15)"
   ]
  },
