    "def fatorial(n):\n",
    "    \"\"\"\n",
    "    -> Retorna o fatorial de um número\n",
    "    param n: Número para fazer o fatorial\n",
    "    return: Fatorial do numero n\n",
    "    \"\"\"\n",
    "    valor = 1\n",
    "    for c in range(1, n+1):\n",
    "        valor *= c\n",
    "    return valor\n",
    "\n",
    "def dobro(n):\n",
    "    return n*2\n",
    "\n",
    "def triplo(n):\n",
    "    return n*3"
   ]
  }
